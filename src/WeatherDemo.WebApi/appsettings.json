{
  "OpenWeatherMap": {
    "BaseUrl": "https://api.openweathermap.org",
    "ApiKey": "place API Key here"
  },
  "WeatherInfoCache": {
    "ExpirationSeconds": 180,
    "SizeLimit": 1024
  },
  "SqliteDbContext": {
    "ConnectionString": "Data Source=Database/WeatherDemo.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
